<qosrules>
  <rule>
    <booleanFilter>
      <equality>
	<attribute>
	  <attributeId>/sys/power/wait_for_fb_status</attributeId>
	  <attributeValue>
	    <string>on</string>
	  </attributeValue>
	</attribute>
      </equality>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/ondemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>25000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>
  <rule>
    <booleanFilter>
      <equality>
	<attribute>
	  <attributeId>/sys/power/wait_for_fb_status</attributeId>
	  <attributeValue>
	    <string>off</string>
	  </attributeValue>
	</attribute>
      </equality>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/ondemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>700000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>
 </qosrules>
