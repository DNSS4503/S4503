# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                                 <mnt_point>       <type>  <mnt_flags and options>                 <fs_mgr_flags>
/dev/block/mmcblk0p12                       /system           ext4     rw,barrier=1                                       wait
/dev/block/mmcblk0p15                       /cache            ext4     nosuid,nodev,barrier=1                             wait,check
/dev/block/mmcblk0p14                       /persist          ext4     nosuid,nodev,barrier=1                             wait,check
/dev/block/mmcblk0p13                       /data             ext4     nosuid,nodev,noauto_da_alloc,barrier=1             wait,check,encryptable=footer
/dev/block/mmcblk0p8                       /boot             emmc     defaults                                           defaults
/dev/block/mmcblk0p16                       /recovery         emmc     defaults                                           defaults
/devices/platform/msm_sdcc.1/mmc_host*       /storage/sdcard0  auto     defaults                                           voldmanaged=sdcard:auto	

